{
  "name": "td-tree-explorer",
  "version": "0.0.1",
  "description": "A reusable and configurable tree component to use within Teradata applications",
  "keywords": [
    "angular",
    "tree",
    "explorer",
    "adapter-pattern",
    "virtual-scroll",
    "material-design"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "bugs": {
    "url": "https://github.com"
  },
  "homepage": "https://github.com/repo#readme",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm -r --filter @tree-core --workspace-concurrency=1 --aggregate-output run build && pnpm -r --filter @tree-explorer --workspace-concurrency=1 --aggregate-output run build",
    "build:lit": "pnpm -r --filter @lit-tree-explorer --aggregate-output run build",
    "test": "pnpm -r --filter @tree-core --filter @tree-explorer --workspace-concurrency=2 --aggregate-output --if-present run test",
    "lint": "pnpm -r --filter @tree-core --filter @tree-explorer --workspace-concurrency=3 --aggregate-output --if-present run lint",
    "typecheck": "pnpm -r --filter @tree-core --filter @tree-explorer --workspace-concurrency=2 --aggregate-output --if-present run typecheck",
    "storybook": "pnpm -r --filter @tree-explorer --aggregate-output run storybook",
    "storybook:build": "pnpm -r --filter @tree-explorer --aggregate-output run storybook:build",
    "storybook:lit": "pnpm -r --filter @lit-tree-explorer --aggregate-output run storybook",
    "storybook:build:lit": "pnpm -r --filter @lit-tree-explorer --aggregate-output run storybook:build",
    "storybook:all": "pnpm -r --parallel --filter @tree-explorer --filter @lit-tree-explorer --aggregate-output run storybook",
    "storybook:build:all": "pnpm -r --filter @tree-explorer --filter @lit-tree-explorer --aggregate-output run storybook:build",
    "docs:check": "node ./scripts/docs-check.mjs",
    "clean": "pnpm -r --if-present run clean && rm -rf dist out-tsc storybook-static .angular",
    "dev": "pnpm storybook"
  },
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@compodoc/compodoc": "1.1.26",
    "@egjs/hammerjs": "^2.0.17",
    "@storybook/addon-docs": "9.0.8",
    "@storybook/addon-onboarding": "9.0.8",
    "@storybook/angular": "9.0.8",
    "@storybook/web-components": "9.0.8",
    "@storybook/web-components-vite": "9.0.8",
    "@testing-library/dom": "^10.4.0",
    "@types/react": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "component-emitter": "^2.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "keycharm": "^0.4.0",
    "ng-packagr": "^19.2.0",
    "puppeteer": "^24.4.0",
    "storybook": "9.0.8",
    "typescript": "~5.6.3",
    "vis-data": "^8.0.3",
    "vis-util": "^6.0.0",
    "vite": "^5.4.12",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "pnpm": {
    "overrides": {
      "@angular-devkit/core": "19.2.15"
    }
  }
}
