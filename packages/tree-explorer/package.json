{
  "name": "@tree-explorer",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./fesm2022/tree-explorer.mjs",
  "module": "./fesm2022/tree-explorer.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "build": "ngc -p tsconfig.lib.json",
    "test": "ng test tree-explorer --watch=false --browsers=ChromeHeadless --no-progress",
