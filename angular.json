{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "tree-core": {
      "projectType": "library",
      "root": "packages/tree-core",
      "sourceRoot": "packages/tree-core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/tree-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/tree-core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/tree-core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/tree-core/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "tree-explorer": {
      "projectType": "library",
      "root": "packages/tree-explorer",
      "sourceRoot": "packages/tree-explorer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/tree-explorer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/tree-explorer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/tree-explorer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/tree-explorer/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "packages/tree-explorer/.storybook",
            "browserTarget": "tree-explorer:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "packages/tree-explorer"
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "packages/tree-explorer/.storybook",
            "browserTarget": "tree-explorer:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "packages/tree-explorer"
            ],
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}